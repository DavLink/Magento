<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="sales_order_shipment_track_save_after">
        <observer name="track_shipment_add" instance="Sbmedical\TrackingAgent\Observer\AddAgent" />
    </event>
    <event name="sales_order_shipment_track_delete_after">
        <observer name="track_shipment_delete" instance="Sbmedical\TrackingAgent\Observer\RemoveAgent" />
    </event>
 </config>